include "square-jar-class.robray"
include "table.robray"



frame pickle_jar {
    //set initial position of pickle jar in center of front table
    parent front_table;
    translation [0, 0, 0.1];
    rpy [-90*deg, 0, 0];

     geometry {
         isa jar;
     }
 }

frame pickle_lid {
    //set initial position of pickle lid in center of front table on top of pickle jar
    parent front_table;
    translation [0, 0, .16];
    
    geometry {
       isa lid;
    }
}