include "square-jar-class.robray"
include "table.robray"



frame pickle_jar {
    //set final location of pickle jar relative to front table
    parent front_table;
    translation [0, 0, 0.1];
    rpy [-90*deg, 0, 0];
    
    geometry {
         isa jar;
     }
 }

frame pickle_lid {
    //set final location of pickle lid relative to front table
    parent front_table;
    translation [-.3, -0.1, 0]; 

    geometry {
        isa lid;
    }
}