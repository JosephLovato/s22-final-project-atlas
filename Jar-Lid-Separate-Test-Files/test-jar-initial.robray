include "square-jar-class.robray"
include "table.robray"



frame pickle_jar {
     parent front_table;
     rpy[-90*deg, 0, 0];
     translation [0, 0, 0.1];

     geometry {
         isa jar;
     }
 }

 frame pickle_lid {
     parent front_table;
     translation [-0.2, -0.1, 0];
     
     geometry {
         isa lid;
     }
 }